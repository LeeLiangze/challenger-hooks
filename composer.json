{
    "name": "chg/challenger-hooks",
    "description": "Hooks integrated in Voyager",
    "keywords": ["laravel", "hooks", "voyager"],
    "license": "MIT",
    "require": {
        "larapack/hooks": "~1.0.2"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "CHG\\ChallengerHooks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CHG\\ChallengerHooks\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CHG\\ChallengerHooks\\VoyagerHooksServiceProvider"
            ]
        }
    }
}
